{
  "fields": {
    "order_id": {
      "type": "uuid"
    },
    "customer_name": {
      "type": "name"
    },
    "customer_email": {
      "type": "email"
    },
    "customer_phone": {
      "type": "phone"
    },
    "shipping_address": {
      "type": "address"
    },
    "billing_address": {
      "type": "address"
    },
    "product_name": {
      "type": "text",
      "max_chars": 100
    },
    "product_category": {
      "type": "choice",
      "options": ["Electronics", "Clothing", "Books", "Home & Garden", "Sports", "Beauty", "Automotive", "Toys", "Food", "Health"]
    },
    "product_price": {
      "type": "float",
      "min": 1.0,
      "max": 2000.0,
      "precision": 2
    },
    "quantity": {
      "type": "integer",
      "min": 1,
      "max": 50
    },
    "total_amount": {
      "type": "float",
      "min": 5.0,
      "max": 10000.0,
      "precision": 2
    },
    "order_date": {
      "type": "date",
      "start": "2020-01-01",
      "end": "2025-01-01"
    },
    "shipping_date": {
      "type": "date",
      "start": "2020-01-01",
      "end": "2025-01-01"
    },
    "delivery_date": {
      "type": "date",
      "start": "2020-01-01",
      "end": "2025-01-01"
    },
    "payment_method": {
      "type": "choice",
      "options": ["Credit Card", "Debit Card", "PayPal", "Apple Pay", "Google Pay", "Bank Transfer", "Cash on Delivery"]
    },
    "order_status": {
      "type": "choice",
      "options": ["Pending", "Confirmed", "Processing", "Shipped", "Delivered", "Cancelled", "Returned"]
    },
    "customer_rating": {
      "type": "float",
      "min": 1.0,
      "max": 5.0,
      "precision": 1
    },
    "review_text": {
      "type": "text",
      "max_chars": 500
    },
    "store_location": {
      "type": "city"
    },
    "salesperson": {
      "type": "name"
    },
    "warehouse_location": {
      "type": "text",
      "max_chars": 50
    },
    "promo_code": {
      "type": "text",
      "max_chars": 8
    },
    "refund_requested": {
      "type": "boolean"
    },
    "delivery_distance_km": {
      "type": "float",
      "min": 0.1,
      "max": 500.0,
      "precision": 1
    },
    "coupon_discount_pct": {
      "type": "integer",
      "min": 0,
      "max": 100
    }
  }
} 